{
  "name": "Moodle",
  "version": "0.0.6",
  "manifest_version": 2,
  "description": "Moodle",
  "homepage_url": "http://www.jayantarora.com",
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js",
      "js/jquery/jquery.min.js",
      "js/ical/ical.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "cookies",
    "notifications",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
